{
  "name": "@workday/canvas-system-icons-web",
  "version": "1.0.41",
  "description": "Workday system icons for web",
  "license": "CC-BY-ND-4.0",
  "author": "Workday, Inc. (https://www.workday.com)",
  "contributors": [
    "Joseph L."
  ],
  "main": "dist/commonjs/index.js",
  "module": "dist/es6/index.js",
  "sideEffects": false,
  "types": "dist/es6/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "clean:build": "rm -rf ./tmp",
    "clean:dist": "rm -rf ./dist",
    "clean": "run-p clean:*",
    "process": "process-svgs @workday/canvas-system-icons -o ./tmp/process -c process-svgs.config.json",
    "optimize": "optimize-svgs ./tmp/process -o ./dist/svg -c svgo.config.json",
    "build:icons": "run-s clean:build process optimize",
    "build:js": "node build.js",
    "build:list": "generate-icon-list ./dist/svg ./dist/asset-list/wd-system-icons-list.json",
    "build:sprite": "svg-sprite -C svg-sprite.config.json --ds ./wd-system-icon-sprite dist/svg/**/*.svg",
    "build": "run-s build:icons build:list && run-p build:js build:sprite clean:build",
    "rebuild": "run-s clean build",
    "test": "jest"
  },
  "dependencies": {
    "@workday/design-assets-types": "0.2.8"
  },
  "devDependencies": {
    "@workday/canvas-colors-web": "2.0.0",
    "@workday/canvas-system-icons": "1.27.0",
    "@workday/icon-utils": "0.25.16",
    "svg-sprite": "^1.3.7"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "gitHead": "4c5d3a1d75832626cb6d086dd0c5837cc9296b7b"
}
