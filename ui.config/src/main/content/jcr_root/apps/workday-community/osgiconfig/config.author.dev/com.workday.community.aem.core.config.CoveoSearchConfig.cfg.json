
{
    "tokenApi":"$[env:coveoSearchEndpoint;default=\"https://platform.cloud.coveo.com/rest/search/v2/token\"]",
    "searchFieldLookupApi": "$[env:coveoSearchFieldEndpoint;default=\"https://platform.cloud.coveo.com/rest/search/values\"]",
    "tokenApiKey":"$[secret:coveoSearchApiKey]",
    "recommendationApiKey":"${secret:coveoSearchRecommendationApiKey]",
    "upcomingEventApiKey":"${secret:coveoSearchUpcomingEventApiKey]",
    "orgId":"$[env:coveoSearchOrgId]",
    "searchHub": "$[env:coveoSearchSearchHub]",
    "defaultEmail":"$[env:coveoSearchDefaultEmail]",
