<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="cq:Page">
    <jcr:content
        cq:deviceGroups="[mobile/groups/responsive]"
        cq:template="/conf/community/settings/wcm/templates/reference"
        jcr:primaryType="cq:PageContent"
        sling:resourceType="community/components/referencepage">
        <root
            jcr:primaryType="nt:unstructured"
            sling:resourceType="community/components/container"
            layout="responsiveGrid">
            <header
                jcr:primaryType="nt:unstructured"
                sling:resourceType="community/components/commons/header"/>
            <container
                jcr:primaryType="nt:unstructured"
                sling:resourceType="community/components/container"
                id="main-container"
                layout="responsiveGrid">
                <breadcrumb
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="community/components/breadcrumb"
                    disableShadowing="false"
                    hideCurrent="false"
                    showHidden="false"/>
                <container
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="community/components/container"
                    id="reference-body-root"
                    layout="responsiveGrid">
                    <container_leftrail
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="community/components/container">
                        <cq:responsive jcr:primaryType="nt:unstructured">
                            <default
                                jcr:primaryType="nt:unstructured"
                                offset="0"
                                width="3"/>
                            <phone
                                jcr:primaryType="nt:unstructured"
                                offset="0"
                                width="12"/>
                        </cq:responsive>
                        <toc
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="community/components/toc"/>
                    </container_leftrail>
                    <container_center
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="community/components/container"
                        id="reference-center-container"
                        layout="responsiveGrid">
                        <container_metadata
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="community/components/container">
                            <cq:responsive jcr:primaryType="nt:unstructured">
                                <default
                                    jcr:primaryType="nt:unstructured"
                                    offset="0"
                                    width="6"/>
                                <phone
                                    jcr:primaryType="nt:unstructured"
                                    offset="0"
                                    width="12"/>
                            </cq:responsive>
                            <title
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="community/components/title"/>
                            <eventmetadata
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="community/components/events/eventmetadata"/>
                        </container_metadata>
                        <container_main
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="community/components/container"
                            editable="{Boolean}true">
                            <cq:responsive jcr:primaryType="nt:unstructured">
                                <default
                                    jcr:primaryType="nt:unstructured"
                                    offset="0"
                                    width="6"/>
                                <phone
                                    jcr:primaryType="nt:unstructured"
                                    offset="0"
                                    width="12"/>
                            </cq:responsive>
                        </container_main>
                        <cq:responsive jcr:primaryType="nt:unstructured">
                            <default
                                jcr:primaryType="nt:unstructured"
                                offset="0"
                                width="6"/>
                            <phone
                                jcr:primaryType="nt:unstructured"
                                offset="0"
                                width="12"/>
                        </cq:responsive>
                    </container_center>
                    <container_rightrail
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="community/components/container">
                        <cq:responsive jcr:primaryType="nt:unstructured">
                            <default
                                jcr:primaryType="nt:unstructured"
                                offset="0"
                                width="3"/>
                            <phone
                                jcr:primaryType="nt:unstructured"
                                offset="0"
                                width="12"/>
                        </cq:responsive>
                        <taxonomy
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="community/components/taxonomy"/>
                    </container_rightrail>
                </container>
            </container>
            <footer
                jcr:primaryType="nt:unstructured"
                sling:resourceType="community/components/commons/footer"/>
        </root>
        <cq:responsive jcr:primaryType="nt:unstructured">
            <breakpoints jcr:primaryType="nt:unstructured">
                <phone
                    jcr:primaryType="nt:unstructured"
                    title="Smaller Screen"
                    width="{Long}768"/>
            </breakpoints>
        </cq:responsive>
    </jcr:content>
</jcr:root>
