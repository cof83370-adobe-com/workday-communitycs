<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="cq:Page">
    <jcr:content
        cq:template="/conf/community/settings/wcm/templates/event-page-template"
        jcr:primaryType="cq:PageContent"
        sling:resourceType="community/components/eventspage">
        <root
            jcr:primaryType="nt:unstructured"
            sling:resourceType="community/components/container"
            layout="responsiveGrid">
            <container
                jcr:primaryType="nt:unstructured"
                sling:resourceType="community/components/container"
                layout="responsiveGrid">
                <eventregistration
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="community/components/container">
                    <button
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Register for Event"
                        sling:resourceType="community/components/button"
                        linkTarget="_self"/>
                </eventregistration>
                <toprightcontainer
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="community/components/container">
                    <image
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="community/components/image"/>
                    <eventdetails
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="community/components/events/eventdetails"/>
                </toprightcontainer>
                <centercontainer
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="community/components/container"/>
                <bottomleftcontainer
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="community/components/container"/>
                <bottomrightcontainer
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="community/components/container"/>
            </container>
        </root>
    </jcr:content>
</jcr:root>
