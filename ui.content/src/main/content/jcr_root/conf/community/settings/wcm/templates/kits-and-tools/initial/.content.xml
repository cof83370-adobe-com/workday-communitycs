<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="cq:Page">
    <jcr:content
        cq:template="/conf/community/settings/wcm/templates/kits-and-tools"
        jcr:primaryType="cq:PageContent"
        sling:resourceType="community/components/kitsandtoolspage">
        <root
            jcr:primaryType="nt:unstructured"
            sling:resourceType="community/components/container"
            layout="responsiveGrid">
            <container
                jcr:primaryType="nt:unstructured"
                sling:resourceType="community/components/container"
                layout="responsiveGrid">
                <container
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="community/components/container"
                    id="kits-and-tools-body-root"
                    layout="responsiveGrid">
                    <container_center
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="community/components/container"
                        id="kits-and-tools-center-container"
                        layout="responsiveGrid">
                        <container_main
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="community/components/container">
                            <text
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="community/components/text"/>
                            <accordion
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="community/components/accordion"/>
                        </container_main>
                    </container_center>
                </container>
            </container>
        </root>
    </jcr:content>
</jcr:root>
