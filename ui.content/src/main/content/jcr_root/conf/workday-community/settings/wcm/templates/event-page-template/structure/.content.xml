<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="cq:Page">
    <jcr:content
        cq:deviceGroups="[mobile/groups/responsive]"
        cq:template="/conf/workday-community/settings/wcm/templates/event-page-template"
        jcr:primaryType="cq:PageContent"
        sling:resourceType="workday-community/components/structure/page">
        <root
            jcr:primaryType="nt:unstructured"
            sling:resourceType="workday-community/components/core/container"
            layout="responsiveGrid">
            <header
                jcr:primaryType="nt:unstructured"
                sling:resourceType="workday-community/components/core/react/header"/>
            <container
                jcr:primaryType="nt:unstructured"
                sling:resourceType="workday-community/components/core/container"
                id="main-container"
                layout="responsiveGrid">
                <breadcrumb
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="workday-community/components/core/breadcrumb"
                    disableShadowing="false"
                    hideCurrent="false"
                    showHidden="false"/>
                <taxonomybadge
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="workday-community/components/common/taxonomybadge"/>
                <titlecontainer
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="workday-community/components/core/container">
                    <title
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="workday-community/components/core/title"/>
                    <eventmetadata
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="workday-community/components/common/eventmetadata"/>
                    <cq:responsive jcr:primaryType="nt:unstructured">
                        <default
                            jcr:primaryType="nt:unstructured"
                            offset="0"
                            width="9"/>
                        <phone
                            jcr:primaryType="nt:unstructured"
                            offset="0"
                            width="12"/>
                    </cq:responsive>
                </titlecontainer>
                <eventregistration
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="workday-community/components/core/container"
                    editable="{Boolean}true">
                    <cq:responsive jcr:primaryType="nt:unstructured">
                        <default
                            jcr:primaryType="nt:unstructured"
                            offset="0"
                            width="3"/>
                        <phone
                            jcr:primaryType="nt:unstructured"
                            offset="0"
                            width="12"/>
                    </cq:responsive>
                </eventregistration>
                <toprightcontainer
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="workday-community/components/core/container"
                    editable="{Boolean}true"/>
                <centercontainer
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="workday-community/components/core/container"
                    editable="{Boolean}true"/>
                <bottomleftcontainer
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="workday-community/components/core/container"
                    editable="{Boolean}true">
                    <cq:responsive jcr:primaryType="nt:unstructured">
                        <default
                            jcr:primaryType="nt:unstructured"
                            offset="0"
                            width="9"/>
                        <phone
                            jcr:primaryType="nt:unstructured"
                            offset="0"
                            width="12"/>
                    </cq:responsive>
                </bottomleftcontainer>
                <bottomrightcontainer
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="workday-community/components/core/container"
                    editable="{Boolean}true">
                    <cq:responsive jcr:primaryType="nt:unstructured">
                        <default
                            jcr:primaryType="nt:unstructured"
                            offset="0"
                            width="3"/>
                        <phone
                            jcr:primaryType="nt:unstructured"
                            offset="0"
                            width="12"/>
                    </cq:responsive>
                </bottomrightcontainer>
            </container>
            <footer
                jcr:primaryType="nt:unstructured"
                sling:resourceType="workday-community/components/core/react/footer"/>
        </root>
        <cq:responsive jcr:primaryType="nt:unstructured">
            <breakpoints jcr:primaryType="nt:unstructured">
                <phone
                    jcr:primaryType="nt:unstructured"
                    title="Smaller Screen"
                    width="{Long}768"/>
                <tablet
                    jcr:primaryType="nt:unstructured"
                    title="Tablet"
                    width="{Long}1200"/>
            </breakpoints>
        </cq:responsive>
    </jcr:content>
</jcr:root>
