<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="cq:Page">
    <jcr:content
        cq:deviceGroups="[mobile/groups/responsive]"
        cq:template="/conf/workday-community/settings/wcm/templates/reference"
        jcr:primaryType="cq:PageContent"
        sling:resourceType="workday-community/components/structure/referencepage">
        <root
            jcr:primaryType="nt:unstructured"
            sling:resourceType="workday-community/components/core/container"
            layout="responsiveGrid">
            <header
                jcr:primaryType="nt:unstructured"
                sling:resourceType="workday-community/components/react/header"/>
            <container
                jcr:primaryType="nt:unstructured"
                sling:resourceType="workday-community/components/core/container"
                id="mainContentId"
                layout="responsiveGrid">
                <breadcrumb
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="workday-community/components/core/breadcrumb"
                    disableShadowing="false"
                    hideCurrent="false"
                    showHidden="false"/>
                <container
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="workday-community/components/core/container"
                    id="reference-body-root"
                    layout="responsiveGrid">
                    <container_leftrail
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="workday-community/components/core/container">
                        <cq:responsive jcr:primaryType="nt:unstructured">
                            <default
                                jcr:primaryType="nt:unstructured"
                                offset="0"
                                width="3"/>
                            <phone
                                jcr:primaryType="nt:unstructured"
                                offset="0"
                                width="12"/>
                        </cq:responsive>
                        <toc
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="workday-community/components/common/toc"/>
                    </container_leftrail>
                    <container_center
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="workday-community/components/core/container"
                        id="center-container"
                        layout="responsiveGrid">
                        <container_metadata
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="workday-community/components/core/container">
                            <cq:responsive jcr:primaryType="nt:unstructured">
                                <default
                                    jcr:primaryType="nt:unstructured"
                                    offset="0"
                                    width="6"/>
                                <phone
                                    jcr:primaryType="nt:unstructured"
                                    offset="0"
                                    width="12"/>
                            </cq:responsive>
                            <title
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="workday-community/components/core/title"/>
                            <metadata
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="workday-community/components/common/metadata"/>
                        </container_metadata>
                        <container_main
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="workday-community/components/core/container"
                            editable="{Boolean}true">
                            <cq:responsive jcr:primaryType="nt:unstructured">
                                <default
                                    jcr:primaryType="nt:unstructured"
                                    offset="0"
                                    width="6"/>
                                <phone
                                    jcr:primaryType="nt:unstructured"
                                    offset="0"
                                    width="12"/>
                            </cq:responsive>
                        </container_main>
                        <cq:responsive jcr:primaryType="nt:unstructured">
                            <default
                                jcr:primaryType="nt:unstructured"
                                offset="0"
                                width="6"/>
                            <phone
                                jcr:primaryType="nt:unstructured"
                                offset="0"
                                width="12"/>
                        </cq:responsive>
                        <container
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="workday-community/components/core/container"
                            id="toc-navigation">
                            <button
                                jcr:primaryType="nt:unstructured"
                                jcr:title="Previous"
                                sling:resourceType="workday-community/components/core/button"
                                id="toc-previous"
                                linkTarget="_self"/>
                            <button_517783785
                                jcr:primaryType="nt:unstructured"
                                jcr:title="Next"
                                sling:resourceType="workday-community/components/core/button"
                                id="toc-next"
                                linkTarget="_self"/>
                        </container>
                    </container_center>
                    <container_rightrail
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="workday-community/components/core/container">
                        <cq:responsive jcr:primaryType="nt:unstructured">
                            <default
                                jcr:primaryType="nt:unstructured"
                                offset="0"
                                width="3"/>
                            <phone
                                jcr:primaryType="nt:unstructured"
                                offset="0"
                                width="12"/>
                        </cq:responsive>
                        <taxonomy
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="workday-community/components/common/taxonomy"/>
                        <relatedinformation
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="workday-community/components/common/relatedinformation"
                            editable="{Boolean}true"/>
                    </container_rightrail>
                </container>
            </container>
            <footer
                jcr:primaryType="nt:unstructured"
                sling:resourceType="workday-community/components/react/footer"/>
        </root>
        <cq:responsive jcr:primaryType="nt:unstructured">
            <breakpoints jcr:primaryType="nt:unstructured">
                <phone
                    jcr:primaryType="nt:unstructured"
                    title="Smaller Screen"
                    width="{Long}768"/>
            </breakpoints>
        </cq:responsive>
    </jcr:content>
</jcr:root>
