<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="cq:Page">
    <jcr:content
        cq:template="/conf/workday-community/settings/wcm/templates/faq"
        jcr:primaryType="cq:PageContent"
        sling:resourceType="workday-community/components/structure/faqpage">
        <root
            jcr:primaryType="nt:unstructured"
            sling:resourceType="workday-community/components/core/container"
            layout="responsiveGrid">
            <container
                jcr:primaryType="nt:unstructured"
                sling:resourceType="workday-community/components/core/container"
                layout="responsiveGrid">
                <container jcr:primaryType="nt:unstructured">
                    <container_center jcr:primaryType="nt:unstructured">
                        <container_main
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="workday-community/components/core/container">
                            <text
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="workday-community/components/core/text"/>
                            <accordion
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="workday-community/components/core/accordion"
                                headingElement="h4"
                                singleExpansion="false">
                                <item_1
                                    cq:panelTitle="Question 1"
                                    jcr:primaryType="nt:unstructured"
                                    jcr:title="Item 1"
                                    sling:resourceType="workday-community/components/core/container"
                                    layout="responsiveGrid">
                                    <text
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="workday-community/components/core/text"
                                        textIsRich="true"/>
                                </item_1>
                                <item_2
                                    cq:panelTitle="Question 2"
                                    jcr:primaryType="nt:unstructured"
                                    jcr:title="Item 2"
                                    sling:resourceType="workday-community/components/core/container"
                                    layout="responsiveGrid">
                                    <text
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="workday-community/components/core/text"
                                        textIsRich="true"/>
                                </item_2>
                            </accordion>
                        </container_main>
                    </container_center>
                    <container_rightrail
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="workday-community/components/core/container">
                        <relatedinformation
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="workday-community/components/common/relatedinformation"/>
                    </container_rightrail>
                </container>
            </container>
        </root>
    </jcr:content>
</jcr:root>
