<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:sling="http://sling.apache.org/jcr/sling/1.0"
    jcr:primaryType="cq:Page">
    <jcr:content
        cq:template="/conf/workday-community/settings/wcm/templates/events"
        jcr:primaryType="cq:PageContent"
        sling:resourceType="workday-community/components/structure/eventspage"
        contentType="[content-types:event]">
        <root
            jcr:primaryType="nt:unstructured"
            sling:resourceType="workday-community/components/core/container"
            layout="responsiveGrid">
            <container
                jcr:primaryType="nt:unstructured"
                sling:resourceType="workday-community/components/core/container"
                layout="responsiveGrid">
                <eventregistration
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="workday-community/components/core/container">
                    <button
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Register for Event"
                        sling:resourceType="workday-community/components/core/button"
                        linkTarget="_self"/>
                </eventregistration>
                <eventdetailscontainer
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="workday-community/components/core/container">
                    <image
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="workday-community/components/core/image"/>
                    <eventdetails
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="workday-community/components/content/events/eventdetails"/>
                </eventdetailscontainer>
                <centercontainer
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="workday-community/components/core/container">
                    <eventdescription
                        cq:styleIds="[1683930780963]"
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="workday-community/components/core/container">
                        <title
                            jcr:primaryType="nt:unstructured"
                            jcr:title="Event Description"
                            sling:resourceType="workday-community/components/core/title"
                            linkTarget="_blank"
                            type="h2"/>
                        <text
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="workday-community/components/core/text"/>
                    </eventdescription>
                    <preparation
                        cq:styleIds="[1683930889156]"
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="workday-community/components/core/container">
                        <title
                            jcr:primaryType="nt:unstructured"
                            jcr:title="Preparation"
                            sling:resourceType="workday-community/components/core/title"
                            linkTarget="_self"
                            type="h2"/>
                        <text
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="workday-community/components/core/text"/>
                    </preparation>
                    <agenda
                        cq:styleIds="[1683930920623]"
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="workday-community/components/core/container">
                        <title
                            jcr:primaryType="nt:unstructured"
                            jcr:title="Agenda"
                            sling:resourceType="workday-community/components/core/title"
                            linkTarget="_self"
                            type="h2"/>
                        <text
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="workday-community/components/core/text"/>
                    </agenda>
                </centercontainer>
                <bottomcontainer jcr:primaryType="nt:unstructured">
                    <bottomrightcontainer
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="workday-community/components/core/container">
                        <speakers
                            cq:styleIds="[1672945911947]"
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="workday-community/components/core/container">
                            <userlist
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="workday-community/components/common/userlist"/>
                        </speakers>
                        <sponsors
                            cq:styleIds="[1672942053585]"
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="workday-community/components/core/container">
                            <title
                                jcr:primaryType="nt:unstructured"
                                jcr:title="Sponsors"
                                sling:resourceType="workday-community/components/core/title"
                                linkTarget="_blank"
                                type="h3"/>
                            <image
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="workday-community/components/core/image"/>
                        </sponsors>
                        <relatedinfo
                            cq:styleIds="[1672945987785]"
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="workday-community/components/core/container">
                            <relatedinformation
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="workday-community/components/common/relatedinformation"
                                footernewTab="_blank"
                                title="Related Information"
                                type="static"/>
                        </relatedinfo>
                    </bottomrightcontainer>
                </bottomcontainer>
            </container>
        </root>
    </jcr:content>
</jcr:root>
