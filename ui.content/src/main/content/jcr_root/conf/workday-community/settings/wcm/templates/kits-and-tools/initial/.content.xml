<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="cq:Page">
    <jcr:content
        cq:template="/conf/workday-community/settings/wcm/templates/kits-and-tools"
        jcr:primaryType="cq:PageContent"
        sling:resourceType="workday-community/components/structure/kitsandtoolspage">
        <root
            jcr:primaryType="nt:unstructured"
            sling:resourceType="workday-community/components/core/container"
            layout="responsiveGrid">
            <container
                jcr:primaryType="nt:unstructured"
                sling:resourceType="workday-community/components/core/container"
                layout="responsiveGrid">
                <container
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="workday-community/components/core/container"
                    id="kits-and-tools-body-root"
                    layout="responsiveGrid">
                    <container_center
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="workday-community/components/core/container"
                        id="kits-and-tools-center-container"
                        layout="responsiveGrid">
                        <container_main
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="workday-community/components/core/container">
                            <text
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="workday-community/components/core/text"/>
                            <accordion
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="workday-community/components/core/accordion"
                                collapseTitle="Collapse"
                                headingElement="h2"
                                singleExpansion="false">
                                <container_recent_updates
                                    cq:panelTitle="Recent Updates"
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="workday-community/components/core/container">
                                    <text
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="workday-community/components/core/text"/>
                                </container_recent_updates>
                                <container_use_case
                                    cq:panelTitle="Use Case"
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="workday-community/components/core/container">
                                    <text
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="workday-community/components/core/text"/>
                                </container_use_case>
                                <container_files
                                    cq:panelTitle="Files"
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="workday-community/components/core/container">
                                    <text
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="workday-community/components/core/text"/>
                                </container_files>
                                <container_prerequisites
                                    cq:panelTitle="Prerequisites"
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="workday-community/components/core/container">
                                    <text
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="workday-community/components/core/text"/>
                                </container_prerequisites>
                                <container_design_notes
                                    cq:panelTitle="Design Notes"
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="workday-community/components/core/container">
                                    <text
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="workday-community/components/core/text"/>
                                </container_design_notes>
                                <container_implementation_notes
                                    cq:panelTitle="Implementation Notes"
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="workday-community/components/core/container">
                                    <text
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="workday-community/components/core/text"/>
                                </container_implementation_notes>
                                <container_steps
                                    cq:panelTitle="Steps"
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="workday-community/components/core/container">
                                    <text
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="workday-community/components/core/text"/>
                                </container_steps>
                            </accordion>
                        </container_main>
                    </container_center>
                    <container_rightrail
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="workday-community/components/core/container">
                        <relatedinformation
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="workday-community/components/common/relatedinformation"
                            title="Related Information"/>
                    </container_rightrail>
                </container>
            </container>
        </root>
    </jcr:content>
</jcr:root>
