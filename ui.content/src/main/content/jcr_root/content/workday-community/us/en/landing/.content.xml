                    jcr:lastModifiedBy="admin"
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="community/components/text"
                    text="&lt;p>Welcome to the Workday Community.&amp;nbsp;Below you’ll find all the resources needed for an introduction to the Workday service and release process.&lt;/p>&#xd;&#xa;"
                    textIsRich="true"/>
                <accordion_1605414754
                    jcr:created="{Date}2022-10-29T02:14:49.850-05:00"
                    jcr:createdBy="admin"
                    jcr:lastModified="{Date}2022-10-31T08:59:40.957-05:00"
