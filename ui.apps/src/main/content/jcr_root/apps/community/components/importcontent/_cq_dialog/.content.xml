<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="Props Config"
    sling:resourceType="cq/gui/components/authoring/dialog">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/coral/foundation/container">
        <items jcr:primaryType="nt:unstructured">
            <tabs
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/tabs"
                maximized="{Boolean}true">
                <items jcr:primaryType="nt:unstructured">
                    <properties
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Properties Config"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <columns
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                                margin="{Boolean}true">
                                <items jcr:primaryType="nt:unstructured">
                                    <column
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                            <choosefile
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                                fieldDescription="Choose the source xml file to import"
                                                fieldLabel="Choose Source File"
                                                name="./chooseSourceFile"
                                                required="{Boolean}true"
                                                rootPath="/content/dam/community"/>
                                            <template
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                                fieldDescription="Choose template based on content type"
                                                fieldLabel="Select Template"
                                                name="./chooseTemplate"
                                                required="{Boolean}true">
                                                <datasource
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="/bin/workday/templateslist/datasource"/>
                                            </template>
                                            <pagepath
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                                fieldDescription="Enter the path where pages to be created."
                                                fieldLabel="Enter Parent Page Path"
                                                name="./pagePath"
                                                required="{Boolean}true"
                                                rootPath="/content/community"/>
                                        </items>
                                    </column>
                                </items>
                            </columns>
                        </items>
                    </properties>
                    <formconfig
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Form Labels Config"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <columns
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                                margin="{Boolean}true">
                                <items jcr:primaryType="nt:unstructured">
                                    <column
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                            <choosefilelabel
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Default value will be 'Given File'"
                                                fieldLabel="Form Source File Label"
                                                name="./chooseSourceFileLabel"/>
                                            <templatelabel
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Default value will be 'Given Template'"
                                                fieldLabel="Form Template Label"
                                                name="./chooseTemplateLabel"/>
                                            <pagepathlabel
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="The default will be 'Given Parent Page Path'"
                                                fieldLabel="Form Page Path Label"
                                                name="./pagePathLabel"/>
                                            <buttonlabel
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Default value will be 'Import Content'"
                                                fieldLabel="Enter Button Label"
                                                name="./buttonLabel"/>
                                        </items>
                                    </column>
                                </items>
                            </columns>
                        </items>
                    </formconfig>
                </items>
            </tabs>
        </items>
    </content>
</jcr:root>
